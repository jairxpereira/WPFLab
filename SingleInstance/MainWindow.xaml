<Window x:Name="ownd" x:Class="SingleInstance.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SingleInstance"
        mc:Ignorable="d"
        Title="MyNotepad" Height="285.638" Width="496.809" Loaded="ownd_onInit">
    <StackPanel>
        <ListBox x:Name="listJanelas" Height="218"/>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="btnNovaJanela" Content="Nova janela" Height="35" Click="btnNovaJanela_onClick"/>
            <Button x:Name="btnFecharJanela" Height="35" Click="btnFecharJanela_onClick">Fechar janela</Button>
            <Button x:Name="btnFecharApp" Height="35" Background="#FFF19797" Click="btnFecharApp_onClick">SAIR</Button>
            
        </StackPanel>
        
    </StackPanel>
</Window>
